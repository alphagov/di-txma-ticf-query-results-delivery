    "@types/node": "18.8.3",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "aws-sdk-client-mock": "1.0.0",
    "aws-sdk-client-mock-jest": "^2.0.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.1",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "jest-when": "3.5.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "yaml-cfn": "0.3.1"
